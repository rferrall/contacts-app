<h1> Contacts Index</h1>

<% @contacts.each do |contact| %>


  <h2> Full Name: 
      <a href="/contacts/<%= contact.id %>">  <%= contact.full_name %> 
      </a>
  </h2>
  <h2> First Name: <%= contact.first_name %> </h2>
  <h2> Middle Name: <%= contact.middle_name %></h2>
  <h2> Last Name: <%= contact.last_name %></h2>
  <h3> Email: <%= contact.email %></h3>
  <h4> Phone Number: <%= contact.phone_number %></h4>
  <h4> Bio: <%= contact.bio %></h4>

<% end %>